  <!-- This is an example for easy recipes. Copy this file into the enderio/config/recipes/user folder to activate it. -->

  <recipe name="RF Gauge" required="true">
    <crafting>
      <grid size="3x3">
        <item name="ingotIron" /><item /><item name="ingotIron" />
