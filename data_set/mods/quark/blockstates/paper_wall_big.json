{
    "multipart": [
        {   "apply": { "model": "quark:block/paper_wall_big_post" }},
        {   "when": { "north": true },
            "apply": { "model": "quark:block/paper_wall_big_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "quark:block/paper_wall_big_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "quark:block/paper_wall_big_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "quark:block/paper_wall_big_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "quark:block/paper_wall_big_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "quark:block/paper_wall_big_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "quark:block/paper_wall_big_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "quark:block/paper_wall_big_noside", "y": 270 }
        }
    ]
}
